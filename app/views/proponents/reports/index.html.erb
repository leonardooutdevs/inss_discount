<%= turbo_stream_from 'proponents_reports' %>
<div class="card mb-3">
  <%= turbo_frame_tag 'proponents_all_reports', target: '_top' do %>
    <div class="row g-0">
      <div class="col-md-12">
        <div class="card-body">
          <div class="text-center mb-5 mt-5">
            <div class="spinner-border" style="width: 10rem; height: 10rem;" role="status">
              <span class="visually-hidden">Loading...</span>
            </div>
          </div>
        </div>
      </div>
    </div>
